{
  "compilerOptions": {
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "noEmit": true,
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "strict": true,
    "strictPropertyInitialization": false
  },
  "include": [".eslintrc.js", "**/*.js", "**/*.ts"],
  "exclude": ["lib", "node_modules"]
}
